{
  "name": "nestjs-api-boilerplate",
  "version": "0.0.1",
  "description": "NestJS API Boilerplate with Auth, Database, Mail, and Redis",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "typeorm": "env-cmd ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm -- --dataSource=src/database/data-source.ts migration:generate",
    "migration:create": "npm run typeorm -- migration:create",
    "migration:run": "npm run typeorm -- --dataSource=src/database/data-source.ts migration:run",
    "migration:revert": "npm run typeorm -- --dataSource=src/database/data-source.ts migration:revert",
    "schema:drop": "npm run typeorm -- --dataSource=src/database/data-source.ts schema:drop",
    "seed:create": "hygen seeds create",
    "seed:run": "ts-node -r tsconfig-paths/register ./src/database/seeds/run-seed.ts",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/src/main",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "env-cmd jest --config ./test/jest-e2e.json",
    "prepare": "is-ci || husky install",
    "db:reset": "pnpm run schema:drop && pnpm run migration:run && pnpm run seed:run"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.312.0",
    "@langchain/community": "^1.1.1",
    "@langchain/core": "^1.1.8",
    "@langchain/openai": "^1.2.0",
    "@nestjs-modules/mailer": "1.8.1",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/cache-manager": "^1.0.0",
    "@nestjs/common": "9.4.0",
    "@nestjs/config": "2.3.1",
    "@nestjs/core": "9.4.0",
    "@nestjs/jwt": "10.0.3",
    "@nestjs/microservices": "^9.4.2",
    "@nestjs/passport": "9.0.3",
    "@nestjs/platform-express": "9.4.0",
    "@nestjs/platform-socket.io": "9.4.0",
    "@nestjs/schedule": "^3.0.4",
    "@nestjs/swagger": "6.3.0",
    "@nestjs/typeorm": "9.0.1",
    "@nestjs/websockets": "9.4.0",
    "@pinecone-database/pinecone": "^6.1.3",
    "@prisma/client": "^7.2.0",
    "@types/aws-sdk": "^2.7.0",
    "@types/ioredis": "^5.0.0",
    "@types/multer-s3": "^3.0.0",
    "apple-signin-auth": "1.7.5",
    "aws-sdk": "^2.1691.0",
    "axios": "^1.7.7",
    "bcryptjs": "2.4.3",
    "cache-manager": "^5.2.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "fb": "2.0.0",
    "google-auth-library": "8.7.0",
    "handlebars": "4.7.7",
    "ioredis": "^5.4.1",
    "langchain": "^1.2.3",
    "mammoth": "^1.11.0",
    "multer": "1.4.4",
    "multer-s3": "3.0.1",
    "nestjs-i18n": "10.2.6",
    "openai": "^4.62.1",
    "passport": "0.6.0",
    "passport-anonymous": "1.0.1",
    "passport-jwt": "4.0.1",
    "passport-linkedin-oauth2": "^2.0.0",
    "pdf-parse": "^2.4.5",
    "pg": "8.10.0",
    "pg-uuid": "^0.14.0",
    "prisma": "^7.2.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "5.0.0",
    "rxjs": "7.8.0",
    "socket.io": "^4.8.3",
    "source-map-support": "0.5.21",
    "swagger-ui-express": "4.6.2",
    "twitter": "1.7.1",
    "undici": "^5.28.4",
    "typeorm": "0.3.15",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^8.57.0",
    "@nestjs/cli": "9.4.1",
    "@nestjs/schematics": "9.1.0",
    "@nestjs/testing": "9.4.0",
    "@types/bcryptjs": "2.4.2",
    "@types/express": "4.17.17",
    "@types/facebook-js-sdk": "3.3.6",
    "@types/jest": "29.5.0",
    "@types/multer": "1.4.7",
    "@types/node": "18.15.13",
    "@types/nodemailer": "^6.4.14",
    "@types/passport-anonymous": "1.0.3",
    "@types/passport-jwt": "3.0.8",
    "@types/supertest": "2.0.12",
    "@types/twitter": "1.7.1",
    "@typescript-eslint/eslint-plugin": "5.59.0",
    "@typescript-eslint/parser": "5.59.0",
    "env-cmd": "10.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^17.0.0",
    "husky": "8.0.3",
    "hygen": "6.2.11",
    "is-ci": "3.0.1",
    "jest": "29.5.0",
    "jiti": "^2.6.1",
    "prettier": "2.8.8",
    "supertest": "6.3.3",
    "ts-jest": "29.1.0",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "tslib": "2.5.0",
    "typescript": "5.0.4",
    "typescript-eslint": "^8.51.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
